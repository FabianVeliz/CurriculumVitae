/* Import library nib */
@import 'nib'

/* Import normalize */
@import 'normalize'

/*Impor fonts*/
@import 'fonts'

/* Box sizing */
*,*:before,*:after
	box-sizing border-box

/* Reset */
h1, h2, h3, h4, h5, h6, ul, li, p
	margin 0
	padding 0
	font-weight normal

ul
	list-style none

a
	text-decoration none

a, img
	display block

html, body
	height 100%

body
	font-size 62.5%
	font-family $roboto
	font-weight 300


